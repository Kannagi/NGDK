WORKDIR = `pwd`

CC = cc

INC = 
CFLAGS = 
RESINC = 
LIBDIR = 
LIB = -lSDL
LDFLAGS = -O3 -s

INC_CONSOLE = $(INC)
CFLAGS_CONSOLE = $(CFLAGS) -O3
RESINC_CONSOLE = $(RESINC)
RCFLAGS_CONSOLE = $(RCFLAGS)
LIBDIR_CONSOLE = $(LIBDIR)
LIB_CONSOLE = $(LIB)
LDFLAGS_CONSOLE = $(LDFLAGS)
OBJDIR_CONSOLE = obj
DEP_CONSOLE = 
OUT_CONSOLE = bin/neogeoextract

OBJ_CONSOLE = main.o

all: $(OUT_CONSOLE)

$(OUT_CONSOLE):  $(OBJ_CONSOLE) $(DEP_CONSOLE)
	mkdir -p bin
	$(CC) $(LIBDIR_CONSOLE) -o $(OUT_CONSOLE) $(OBJ_CONSOLE)  $(LDFLAGS_CONSOLE) $(LIB_CONSOLE)

main.o: main.c
	$(CC) $(CFLAGS_CONSOLE) $(INC_CONSOLE) -c main.c -o main.o

clean: 
	rm -f $(OBJ_CONSOLE) $(OUT_CONSOLE)
	rm -rf bin

.PHONY: clean
